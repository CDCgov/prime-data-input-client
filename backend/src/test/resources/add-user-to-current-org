mutation addUserToCurrentOrg(
    $firstName: String,
    $middleName: String,
    $lastName: String!,
    $suffix: String,
    $email: String!,
    $role: OrganizationRole,
    ){
  addUserToCurrentOrg(
    firstName: $firstName
    middleName: $middleName
    lastName: $lastName
    suffix: $suffix
    email: $email
    role: $role
  ) {
      id,
      firstName,
      middleName,
      lastName,
      suffix,
      email,
      permissions,
      roles,
      facilities {
        name
        id
      }
      organization {
        name
        externalId
        testingFacility {
          name
          id
        }
      }
    }
}
