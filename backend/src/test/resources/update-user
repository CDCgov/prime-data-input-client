mutation updateUser(
    $id: String!,
    $firstName: String,
    $middleName: String,
    $lastName: String!,
    $suffix: String,
    $email: String!,
    ){
  updateUser(
    id: $id,
    firstName: $firstName
    middleName: $middleName
    lastName: $lastName
    suffix: $suffix
    email: $email
  ) {
      id
      firstName,
      middleName,
      lastName,
      suffix,
      email,
      permissions,
      organization {
        name
        externalId
      }
    }
}
