mutation patientLinkSubmit($internalId: String!, $birthDate: LocalDate!, $symptoms: String!, $previousTestDate: LocalDate!, $symptomOnsetDate: LocalDate!) {
    patientLinkSubmit(
      internalId: $internalId, 
      birthDate: $birthDate,
      symptoms: $symptoms,
      priorTestDate: $previousTestDate,
      symptomOnset: $symptomOnsetDate
    )
  }