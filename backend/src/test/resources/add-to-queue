mutation enqueueById($id: String!, $facilityId: String!, $previousTestDate: LocalDate, $symptomOnsetDate: LocalDate) {
    addPatientToQueue(
      patientId: $id,
      facilityId: $facilityId,
      pregnancy: "no",
      symptoms:"{}",
      firstTest: false,
      priorTestDate: $previousTestDate,
      symptomOnset: $symptomOnsetDate
    )
  }