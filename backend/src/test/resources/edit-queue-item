mutation EditQueueItem($id: String!, $deviceId: String, $result: String, $dateTested: String) {
    editQueueItem(
      id: $id,
      deviceId: $deviceId,
      result: $result,
      dateTested: $dateTested
    ) {
      internalId,
      result,
      deviceType {
        internalId
      }
    }
  }