mutation updateDevice(
    $id: String!
    $name: String!
    $manufacturer: String!
    $model: String!
    $loincCode: String!
    $swabType: String!
) {
    updateDeviceType(
      id: $id
      name: $name
      manufacturer: $manufacturer
      model: $model
      loincCode: $loincCode
      swabType: $swabType

    ) {
      internalId
      name
      model
      manufacturer
      loincCode
      swabType
    }
}