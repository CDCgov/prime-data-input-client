mutation updateDevice(
    $id: String!
    $name: String!
    $manufacturer: String!
    $model: String!
    $loincCode: String!
) {
    updateDeviceType(
      id: $id
      name: $name
      manufacturer: $manufacturer
      model: $model
      loincCode: $loincCode
    ) {
      internalId
      name
      model
      manufacturer
      loincCode
    }
}