mutation addDevice(
    $name: String!
    $manufacturer: String!
    $model: String!
    $loincCode: String!
) {
    createDeviceType(
      name: $name
      manufacturer: $manufacturer
      model: $model
      loincCode: $loincCode
    ) {
      internalId
      name
      model
     manufacturer
     loincCode
    }
}